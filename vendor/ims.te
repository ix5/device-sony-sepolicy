type ims, domain;
type ims_exec, exec_type, vendor_file_type, file_type;

init_daemon_domain(ims)

net_domain(ims)

allow ims init:unix_stream_socket connectto;
allow ims property_socket:sock_file write;
allow ims self:socket { create read write };

#allow ims vendor_default_prop:property_service set;
set_prop(ims, vendor_default_prop)
